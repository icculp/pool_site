{
  "background": {
    "color": {
      "value": "#ffffff"
    },
    "position": "50% 50%",
    "repeat": "no-repeat",
    "size": "cover"
  },
  "backgroundMask": {
    "cover": {
      "color": {
        "value": "#cdf4cd"
      }
    }
  },
  "fullScreen": {
    "zIndex": 10
  },
  "fpsLimit": 240,
  "interactivity": {
    "events": {
      "onClick": {
        "enable": true,
        "mode": "push"
      },
      "onDiv": {
        "selectors": "#repulse-div",
        "mode": "repulse"
      },
      "onHover": {
        "enable": true,
        "mode": "repulse",
        "parallax": {
          "force": 20,
          "smooth": 100
        }
      },
      "resize": false
    },
    "modes": {
      "bubble": {
        "distance": 400,
        "duration": 2,
        "opacity": 0.8,
        "size": 40
      },
      "grab": {
        "distance": 400
      },
      "repulse": {
        "distance": 100,
        "duration": 0.3
      }
    }
  },
  "motion": {
    "reduce": {
      "factor": 1,
      "value": false
    }
  },
  "particles": {
    "color": {
      "value": "#0033ad"
    },
    "links": {
      "color": {
        "value": "#0033ad"
      },
      "distance": 150,
      "enable": true,
      "opacity": 0.4
    },
    "move": {
      "attract": {
        "rotate": {
          "x": 600,
          "y": 1200
        }
      },
      "enable": true,
      "path": {},
      "outModes": {
        "bottom": "out",
        "left": "out",
        "right": "out",
        "top": "out"
      },
      "spin": {}
    },
    "number": {
      "density": {
        "enable": true
      },
      "value": 120
    },
    "opacity": {
      "random": {
        "enable": true
      },
      "value": {
        "min": 0.1,
        "max": 0.5
      },
      "animation": {
        "count": 10,
        "enable": true,
        "speed": 3,
        "minimumValue": 0.1
      }
    },
    "size": {
      "random": {
        "enable": true
      },
      "value": {
        "min": 0.1,
        "max": 5
      },
      "animation": {
        "enable": true,
        "speed": 20,
        "sync": true,
        "minimumValue": 0.1
      }
    }
  },
  "zLayers": 10
}